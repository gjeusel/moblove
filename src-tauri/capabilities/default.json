{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://messy.s3.fr-par.scw.cloud"
        },
        {
          "url": "https://cdn.readdetectiveconan.com/*"
        },
        {
          "url": "https://mangapill.com/*"
        }
      ]
    },
    "log:default",
    "fs:default"
  ]
}

